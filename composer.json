{
	"name": "designcontainer/replace_with_repo_name",
	"description": "A Design Container Website",
	"repositories":[
		{
			"type":"composer",
			"url":"https://wpackagist.org"
		},
		{
			"type": "package",
			"package": {
				"name": "advanced-custom-fields/advanced-custom-fields-pro",
				"version": "5.9.5",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://connect.advancedcustomfields.com/v2/plugins/download?p=pro&k={%PLUGIN_ACF_KEY}"
				}
			}
		}
	],
	"require": {
		"advanced-custom-fields/advanced-custom-fields-pro":"*",
		"wpackagist-plugin/autodescription":"*",
		"wpackagist-plugin/wp-nested-pages":"*",
		"wpackagist-plugin/regenerate-thumbnails":"*",
		"ffraenz/private-composer-installer": "^5.0",
		"composer/installers":"v1.0.6"
	},
	"extra": {
		"private-composer-installer": {
			"dotenv-path": ".",
			"dotenv-name": ".env"
		},
		"installer-paths": {
			"wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
			"wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
		}
	}
 }